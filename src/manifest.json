{
  "manifest_version": 3,
  "name": "插件",
  "version": "1.0",
  "description": "插件",
  "permissions": [
    "declarativeContent",
    "browsingData",
    "webRequest",
    "activeTab",
    "tabs",
    "scripting",
    "cookies",
    "storage",
    "downloads",
    "clipboardWrite"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "options_page": "options/index.html",
  "host_permissions": [
    "https://www.baidu.com"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    
    
  ],
  "web_accessible_resources": [
      
  ]
}